    "name": "Join:Simple",
    "query": "select * from orders as o inner join `inventory` as i on o.item=i.sku",
    "type": "aggregate",
    "output": {
      "pipeline": [
        {"$project": {"o": "$$ROOT"}},
        {
          "$lookup": {
            "from": "inventory",
            "as": "i",
            "localField": "o.item",
            "foreignField": "sku"
          }
        },
        {"$match": {"$expr": {"$gt": [{"$size": "$i"}, 0]}}}
      ],
      "collections": ["orders", "inventory"]
    }
