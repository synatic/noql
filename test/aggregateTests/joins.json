            "customer-notes": "$customer-notes",
            "cn2": "$cn2"
          }
        }
      ],
      "collections": ["customers", "customer-notes", "customer-notes2"]
    }
  },
  {
    "name": "Join: with complex on",
    "query": "select c.*,cn.* from customers c inner join `customer-notes` cn on cn.id=c.id and (cn.id>2 or cn.id<5)",
    "type": "aggregate",
    "output": {
      "pipeline": [
        {
          "$project": {
            "c": "$$ROOT"
          }
        },
        {
          "$lookup": {
            "from": "customer-notes",
            "as": "cn",
            "let": {
              "c_id": "$c.id"
            },
            "pipeline": [
              {
                "$match": {
                  "$expr": {
                    "$and": [
                      {
                        "$eq": ["$id", "$$c_id"]
                      },
                      {
                        "$or": [
                          {
                            "$gt": ["$id", 2]
                          },
                          {
                            "$lt": ["$id", 5]
                          }
                        ]
                      }
                    ]
                  }
                }
              }
            ]
          }
        },
        {
          "$match": {
            "$expr": {
              "$gt": [
                {
                  "$size": "$cn"
                },
                0
              ]
            }
          }
        },
        {
          "$project": {
            "c": "$c",
            "cn": "$cn"
          }
        }
      ],
      "collections": ["customers", "customer-notes"]
    }
  },
  {
    "name": "Join: Sub Query with hint",
    "query": "select c.*,cn.* from customers c inner join (select * from `customer-notes` where id>2) `cn|first` on cn.id=c.id",
    "type": "aggregate",
    "output": {
      "pipeline": [
        {
          "$project": {
            "c": "$$ROOT"
          }
        },
        {
          "$lookup": {
            "from": "customer-notes",
            "as": "cn",
            "let": {
              "c_id": "$c.id"
            },
            "pipeline": [
              {
                "$match": {
                  "id": {
                    "$gt": 2
                  }
                }
              },
              {
                "$match": {
                  "$expr": {
                    "$eq": ["$id", "$$c_id"]
                  }
                }
              }
            ]
          }
        },
        {
          "$set": {
            "cn": {
              "$first": "$cn"
            }
          }
        },
        {
          "$match": {
            "cn": {
              "$ne": null
            }
          }
        },
        {
          "$project": {
            "c": "$c",
            "cn": "$cn"
          }
        }
      ],
      "collections": ["customers", "customer-notes"]
    }
