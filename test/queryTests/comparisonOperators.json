  {
    "name": "Comparison:Case",
    "query": "select id,(case when id=3 then '1' when id=2 then '1' else 0 end) as test from `customers`",
    "output": {
      "limit": 100,
      "collection": "customers",
      "projection": {
        "id": "$id",
        "test": {
          "$switch": {
            "branches": [
              {
                "case": {
                  "$eq": ["$id", 3]
                },
                "then": "1"
              },
              {
                "case": {
                  "$eq": ["$id", 2]
                },
                "then": "1"
              }
            ],
            "default": {
              "$literal": 0
            }
          }
        }
      }
    }
